<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Kursach.client.controllers.HomeController">
   <Label fx:id="menuTitle" text="Меню Администратора">
      <font>
         <Font size="52.0" />
      </font>
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
      <VBox.margin>
         <Insets bottom="80.0" top="25.0" />
      </VBox.margin>
   </Label>
   <Button fx:id="profileButton" mnemonicParsing="false" prefWidth="160.0" text="Мой профиль">
      <font>
         <Font size="14.0" />
      </font>
      <VBox.margin>
         <Insets bottom="25.0" />
      </VBox.margin>
   </Button>
   <Button fx:id="orderButton" mnemonicParsing="false" prefWidth="160.0" text="Заказы">
      <VBox.margin>
         <Insets bottom="25.0" />
      </VBox.margin>
      <font>
         <Font size="14.0" />
      </font>
   </Button>
   <Button fx:id="productButton" mnemonicParsing="false" prefWidth="160.0" text="Продукты">
      <font>
         <Font size="14.0" />
      </font>
      <VBox.margin>
         <Insets bottom="25.0" />
      </VBox.margin>
   </Button>
   <Button fx:id="categoryButton" mnemonicParsing="false" prefWidth="160.0" text="Категории продуктов">
      <font>
         <Font size="14.0" />
      </font>
      <VBox.margin>
         <Insets bottom="25.0" />
      </VBox.margin>
   </Button>
   <Button fx:id="manufacturerButton" mnemonicParsing="false" prefWidth="160.0" text="Производители">
      <font>
         <Font size="14.0" />
      </font>
      <VBox.margin>
         <Insets bottom="25.0" />
      </VBox.margin>
   </Button>
   <Button fx:id="providerButton" mnemonicParsing="false" prefWidth="160.0" text="Поставщики" VBox.vgrow="NEVER">
      <font>
         <Font size="14.0" />
      </font>
      <VBox.margin>
         <Insets bottom="25.0" />
      </VBox.margin>
   </Button>
   <Button fx:id="countryButton" mnemonicParsing="false" prefWidth="160.0" text="Страны">
      <font>
         <Font size="14.0" />
      </font>
      <VBox.margin>
         <Insets bottom="25.0" />
      </VBox.margin>
   </Button>
   <Button fx:id="clientButton" mnemonicParsing="false" prefWidth="160.0" text="Клиенты">
      <font>
         <Font size="14.0" />
      </font>
      <VBox.margin>
         <Insets bottom="25.0" />
      </VBox.margin>
   </Button>
   <Button fx:id="usersButton" mnemonicParsing="false" prefWidth="160.0" text="Пользователи">
      <font>
         <Font size="14.0" />
      </font>
      <VBox.margin>
         <Insets bottom="25.0" />
      </VBox.margin>
   </Button>
   <AnchorPane minHeight="-Infinity" prefHeight="60.0" prefWidth="800.0">
      <VBox.margin>
         <Insets bottom="15.0" />
      </VBox.margin>
      <Button fx:id="exitButton" layoutX="14.0" layoutY="15.0" mnemonicParsing="false" text="Выход" AnchorPane.leftAnchor="25.0">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </AnchorPane>
</VBox>
